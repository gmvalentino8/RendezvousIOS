# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'
target 'Rendezvous' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Rendezvous
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  pod 'SDWebImage', '~> 4.0'

    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'GoogleMaps'

  target 'RendezvousTests' do
    inherit! :search_paths
    pod 'Firebase'
    # Pods for testing
  end

  target 'RendezvousUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
